<table cellpadding="10" cellspacing="0" summary="" i18n:domain="cpsskins"
 tal:define="mcat python: request.get('cpsskins_mcat')">
  <tr style="height: 230px">
    <td>
      <div style="text-align: left; width:130px"
      tal:attributes="class string:boxShape${here/title} cpsskinsBox">
        <br />
        <div class="title" style="background-color: #e0e0e0">
          <tal:block define="title string:_Menu title_"
          content="python: mcat and mcat(title) or title"
          i18n:translate="" />
        </div>
        <div class="body" style="background-color: #f0f0f0"
         style="border-style: solid; border-width: 1px; padding: 5px">
          <ul>
            <tal:block repeat="entry python: range(1, 3)">
              <li tal:define="itemclass python: entry == 2 and 'selected' or None"
                  tal:attributes="class itemclass">
                <a href=""
                tal:define="title string:_Menu entry_"
                tal:content="python: mcat and mcat(title) or title"
                i18n:translate="" /></li>
            </tal:block>
          </ul>
        </div>
      </div>
      <br/>
      <div style="text-align: left; width: 280px"
      tal:attributes="class string:boxShape${here/title} cpsskinsTab">
        <div class="title" style="background-color: #e0e0e0">
          <tal:block define="title string:_Menu title_"
          content="python: mcat and mcat(title) or title"
          i18n:translate="" />
        </div>
        <div class="body" style="background-color: #f0f0f0">
          <ul>
            <tal:block repeat="entry python: range(1, 3)">
              <li tal:define="itemclass python: entry == 2 and 'selected' or None"
                  tal:attributes="class itemclass">
                <a href="" style="border-color: Gray; background-color: White"
                tal:define="title string:_Menu entry_"
                tal:content="python: mcat and mcat(title) or title"
                i18n:translate="" />
              </li>
            </tal:block>
          </ul>
        </div>
      </div>
    </td>
  </tr>
</table>
