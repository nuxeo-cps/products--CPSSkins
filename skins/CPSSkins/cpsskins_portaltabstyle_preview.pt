<table cellpadding="10" cellspacing="0" width="100%" summary=""
i18n:domain="cpsskins"
tal:define="mcat python: request.get('cpsskins_mcat')">
  <tr>
    <td>
      <div
      tal:attributes="class string:portalTab${here/title} cpsskinsRoundedTabs">
        <ul>
          <tal:block repeat="entry python: range(1, 4)">
            <li tal:define="itemclass python: entry == 3 and 'selected' or None"
                tal:attributes="class itemclass">
              <a href="">
                <tal:block define="title string:_Menu entry_"
                content="python: mcat and mcat(title) or title"
                i18n:translate="" />
                <tal:block content="entry" />
              </a>
            </li>
          </tal:block>
        </ul>
      </div>
      <br style="clear:left" />
    </td>
  </tr>
</table>
