<div i18n:domain="cpsskins" style="width: 160px"
 tal:define="inline options/inline|nothing;
             mcat python: request.get('cpsskins_mcat')"
 tal:attributes="class string:fontColor${here/title}">
  <h1 tal:define="title string:_H1_font_" i18n:translate=""
  tal:content="python: mcat and mcat(title) or title" />
  <p tal:define="title string:_P_font_" i18n:translate=""
  tal:content="python: mcat and mcat(title) or title" />
  <h2 tal:define="title string:_H2_font_" i18n:translate=""
  tal:content="python: mcat and mcat(title) or title" />
  <strong tal:define="title string:_STRONG_font_"
  i18n:translate=""
  tal:content="python: mcat and mcat(title) or title" />
  <h3 tal:define="title string:_H3_font_" i18n:translate=""
  tal:content="python: mcat and mcat(title) or title" />
  <a href="" tal:define="title string:_A_link_decoration_"
  i18n:translate=""
  tal:content="python: mcat and mcat(title) or title" />
  <h4 tal:define="title string:_H456_font_" i18n:translate=""
  tal:content="python: mcat and mcat(title) or title" />
</div>
