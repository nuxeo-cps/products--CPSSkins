<?xml version="1.0"?>
<object name="Theme Folder" meta_type="Factory-based Type Information"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title"></property>
 <property name="description">A Theme Folder contain theme items.</property>
 <property name="content_icon">themefolder.png</property>
 <property name="content_meta_type">Theme Folder</property>
 <property name="product">CPSSkins</property>
 <property name="factory">addThemeFolder</property>
 <property name="immediate_view"></property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">False</property>
 <property name="allowed_content_types">
  <element value="Area Shape"/>
  <element value="Area Color"/>
  <element value="Portal Box Shape"/>
  <element value="Portal Box Color"/>
  <element value="Font Color"/>
  <element value="Font Shape"/>
  <element value="Collapsible Menu Style"/>
  <element value="Portal Tab Style"/>
  <element value="Form Style"/>
  <element value="Box Corners"/>
  <element value="Calendar Style"/>
  <element value="Palette Color"/>
  <element value="Palette Border"/>
  <element value="Page Block"/>
  <element value="Search Box Templet"/>
  <element value="Action Box Templet"/>
  <element value="Text Box Templet"/>
  <element value="Image Box Templet"/>
  <element value="Flash Box Templet"/>
  <element value="Portal Box Templet"/>
  <element value="Document Info Templet"/>
  <element value="Theme Chooser Templet"/>
  <element value="Language Templet"/>
  <element value="Breadcrumbs Templet"/>
  <element value="Portal Box Group Templet"/>
  <element value="Main Content Templet"/>
  <element value="Collapsible Menu Templet"/>
  <element value="Portal Tab Templet"/>
  <element value="Cell Sizer"/>
  <element value="Cell Styler"/>
  <element value="Cell Hider"/>
  <element value="Cell Block"/>
  <element value="Calendar Templet"/>
  <element value="Portlet Box Templet"/>
  <element value="Portal Theme"/>
  <element value="Theme Page"/>
  <element value="Theme Folder"/>
 </property>
 <property name="allow_discussion">False</property>
 <property name="cps_is_searchable">False</property>
 <property name="cps_proxy_type"></property>
 <property name="cps_display_as_document_in_listing">False</property>
 <alias from="(Default)" to="folder_view"/>
 <alias from="view" to="folder_view"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    url_expr="string:${object_url}/folder_view" visible="True"/>
</object>
