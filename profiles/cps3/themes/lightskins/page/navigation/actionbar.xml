<?xml version="1.0"?>
<object name="actionbar" meta_type="Action Box Templet">
 <property name="title">Action bar</property>
 <property name="cacheable">True</property>
 <property name="cache_lifetime">60</property>
 <property name="hidden_in_text_mode">False</property>
 <property name="display_title_in_text_mode">False</property>
 <property name="xpos">0</property>
 <property name="align">left</property>
 <property name="padding">0.5em</property>
 <property name="margin">0.5em</property>
 <property name="templet_height"></property>
 <property name="fontcolor">Black</property>
 <property name="fontshape">Arial</property>
 <property name="shape"></property>
 <property name="color"></property>
 <property name="formstyle"></property>
 <property name="visibility">always</property>
 <property name="visibility_paths"/>
 <property name="languages"/>
 <property name="esi_fragment">False</property>
 <property name="actions_to_display">
  <element value="global:directories"/>
  <element value="global:configThemes"/>
  <element value="user:login"/>
  <element value="user:logout"/>
  <element value="user:join"/>
  <element value="global:configPortal"/>
  <element value="global:boxes"/>
 </property>
 <property name="style">text and icons</property>
 <property name="orientation">horizontal</property>
</object>
