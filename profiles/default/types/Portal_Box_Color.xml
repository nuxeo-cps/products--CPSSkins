<?xml version="1.0"?>
<object name="Portal Box Color" meta_type="Factory-based Type Information"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title"></property>
 <property name="description">_boxcolor_description_</property>
 <property name="content_icon">box_color.png</property>
 <property name="content_meta_type">Portal Box Color</property>
 <property name="product">CPSSkins</property>
 <property name="factory">addPortalBoxColor</property>
 <property name="immediate_view">cpsskins_edit_form</property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">False</property>
 <property name="allowed_content_types"/>
 <property name="allow_discussion">False</property>
 <property name="cps_is_searchable">False</property>
 <property name="cps_proxy_type"></property>
 <property name="cps_display_as_document_in_listing">False</property>
 <property name="cps_is_portalbox">False</property>
 <alias from="(Default)" to="cpsskins_style_view"/>
 <alias from="delete" to="cpsskins_object_delete"/>
 <alias from="edit" to="cpsskins_edit_form"/>
 <alias from="edit_form" to="cpsskins_edit_form"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    url_expr="string:${object_url}/cpsskins_style_view" visible="True"/>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    url_expr="string:${object_url}/cpsskins_edit_form" visible="True">
  <permission value="Manage Themes"/>
 </action>
 <action title="Delete" action_id="delete" category="object" condition_expr=""
    url_expr="string:${object_url}/cpsskins_object_delete" visible="False">
  <permission value="Manage Themes"/>
 </action>
 <action title="isportalstyle" action_id="isportalstyle" category="object"
    condition_expr="" url_expr="string:${object_url}/isPortalStyle"
    visible="False"/>
</object>
