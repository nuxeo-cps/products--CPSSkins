<?xml version="1.0"?>
<type-info id="Portal Theme"
           kind="Factory-based Type Information">
  <property name="title"></property>
  <property name="description">_portaltheme_description_</property>
  <property name="content_icon">portaltheme.png</property>
  <property name="content_meta_type">Portal Theme</property>
  <property name="product">CPSSkins</property>
  <property name="factory">addPortalTheme</property>
  <property name="immediate_view">cpsskins_edit_form</property>
  <property name="global_allow">0</property>
  <property name="filter_content_types">0</property>
  <property name="allowed_content_types">
   <element value="Theme Page" />
   <element value="Theme Folder" /></property>
  <property name="allow_discussion">False</property>
  <property name="cps_is_searchable">0</property>
  <property name="cps_proxy_type"></property>
  <property name="cps_display_as_document_in_listing">0</property>
  <property name="cps_is_portalbox">0</property>
  <aliases>
   <alias from="edit_palettes" to="cpsskins_edit_palettes"/>
   <alias from="manage_themes" to="cpsskins_themes_manager"/>
   <alias from="(Default)" to="cpsskins_theme_view"/>
   <alias from="edit" to="cpsskins_edit_form"/>
   <alias from="manage_cache" to="cpsskins_cache_manager"/>
   <alias from="edit_styles" to="cpsskins_edit_styles"/>
   <alias from="edit_images" to="cpsskins_edit_images"/>
   <alias from="view" to="cpsskins_theme_view"/>
  </aliases>
  <action action_id="view" title="View" condition_expr=""
          url_expr="string:${object_url}/cpsskins_theme_view"
          category="object" visible="True">
  </action>
  <action action_id="edit" title="_Edit_" condition_expr=""
          url_expr="string:${object_url}/cpsskins_edit_form"
          category="object" visible="False">
   <permission>Manage Themes</permission>
  </action>
  <action action_id="edit_styles" title="_Edit styles_"
          condition_expr=""
          url_expr="string:${object_url}/cpsskins_edit_styles"
          category="object" visible="False">
   <permission>Manage Themes</permission>
  </action>
  <action action_id="edit_palettes" title="_Edit palettes_"
          condition_expr=""
          url_expr="string:${object_url}/cpsskins_edit_palettes"
          category="object" visible="False">
   <permission>Manage Themes</permission>
  </action>
  <action action_id="edit_images" title="_Edit images_"
          condition_expr=""
          url_expr="string:${object_url}/cpsskins_edit_images"
          category="object" visible="False">
   <permission>Manage Themes</permission>
  </action>
  <action action_id="manage_cache" title="_Cache manager_"
          condition_expr=""
          url_expr="string:${object_url}/cpsskins_cache_manager"
          category="object" visible="False">
   <permission>Manage Themes</permission>
  </action>
  <action action_id="manage_themes" title="_Themes manager_"
          condition_expr=""
          url_expr="string:${object_url}/cpsskins_themes_manager"
          category="object" visible="False">
   <permission>Manage Themes</permission>
  </action>
</type-info>
