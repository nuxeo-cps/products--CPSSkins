<?xml version="1.0"?>
<object name="Breadcrumbs Templet" meta_type="Factory-based Type Information"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title"></property>
 <property name="description">_breadcrumbs_templet_description_</property>
 <property name="content_icon">breadcrumbs_templet.png</property>
 <property name="content_meta_type">Breadcrumbs Templet</property>
 <property name="product">CPSSkins</property>
 <property name="factory">addBreadcrumbs</property>
 <property name="immediate_view">cpsskins_edit_form</property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">False</property>
 <property name="allowed_content_types"/>
 <property name="allow_discussion">False</property>
 <property name="cps_is_searchable">False</property>
 <property name="cps_proxy_type"></property>
 <property name="cps_display_as_document_in_listing">False</property>
 <alias from="(Default)" to="index_html"/>
