 <alias from="view" to="cpsskins_templet_view"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    url_expr="string:${object_url}/cpsskins_templet_view" visible="True"/>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    url_expr="string:${object_url}/cpsskins_edit_form" visible="True">
  <permission value="Manage Themes"/>
 </action>
 <action title="isportaltemplet" action_id="isportaltemplet" category="object"
    condition_expr="" url_expr="string:${object_url}/isPortalTemplet"
